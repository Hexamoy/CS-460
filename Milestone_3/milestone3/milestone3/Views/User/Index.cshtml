@model IEnumerable<milestone3.Models.User>

@{
    ViewBag.Title = "Admin Page";
}

@*NOTE: This page is for debugging purposes*@
<h2>User Account List</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pseudonym)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PaymentInformation)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pseudonym)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaymentInformation)
        </td>
    </tr>
}

</table>
